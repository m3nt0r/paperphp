{% extends "layouts/error.twig" %}

{% block title %}Server Exception{% endblock %}

{% block content %}

    <h2>An exception was raised</h2>
    <p>{{ errorMessage }}</p>
    <p><small class="text-muted">Error Code: {{ errorCode }}</small><br>
        <small class="text-muted">Document: {{ request.path }}</small></p>

{% endblock %}